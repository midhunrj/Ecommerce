}

.home-slider {
    margin-top:20px;
    /* height: 100vh; */
    overflow: hidden;
    position: relative;
}

.single-hero-slider {
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    position: relative;
}



.single-hero-slider .row {
    /* height: 100%; */
    position: relative;
    z-index: 2;
}

.single-hero-slider .hero-slider-content-2 {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    z-index: 3;
    color: #fff;
    text-align: center;
}

.single-hero-slider img {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
   object-fit: fill;
    z-index: 1;
}

.single-hero-slider .animated {
    animation: fadeIn 1s ease-in-out;
    color:aliceblue;
    
}

@keyframes fadeIn {
    from {
        opacity: 0;
    }
    to {
        opacity: 1;
    }
}
   </style>
    
