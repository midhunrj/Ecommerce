<div class="row mb-50 mt-20 order-info-wrap text-center">
    <% if(orders&&orders._id){ %>
    <div class="col-md-4">
        <article class="icontext align-items-start">
            <% if(users&&users[j]._id.toString()==orders[i].userId) {%>                              
            <div class="text">
                <h6 class="mb-1">Customer</h6>
                <p class="mb-1">
                    <%= users.username %> <br> <%= users.email %> <br> +<%= users.phone %>
                </p>
                <a href="#">View profile</a>
                <% } %>
            </div>
        </article>
    </div> <!-- col// -->
    <div class="col-md-4">
        <article class="icontext align-items-start">                              
            <div class="text">
                <h6 class="mb-1">Order info</h6>
                <p class="mb-1">
                    Shipping: Fargo express <br> Pay method: <%= orders.paymentstatus %> <br> Status: new
                </p>
                <a href="#">Download info</a>
            </div>
        </article>
    </div> <!-- col// -->
    <div class="col-md-4">
        <article class="icontext align-items-start">                              
            <div class="text">
                <h6 class="mb-1">Deliver to</h6>
                <p class="mb-1">
                    City: <%=orders.Address[0].city %>,<%=orders.Address[0].state %>,<%=orders.Address[0].country %> <br><%=orders.Address[0].address %>,<%=orders.Address[0].landmark %> <br> <%=orders.Address[0].country %> <%=orders.Address[0].pincode %>
                </p>
                <a href="#">View profile</a>
            </div>
        </article>
    </div> <!-- col// -->
</div> <!-- row // -->
<% } %>